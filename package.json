{
  "name": "orm-demo",
  "version": "0.0.1",
  "description": "Express Typescript demo app",
  "devDependencies": {
    "@types/node": "^9.4.5",
    "nodemon": "^1.14.12",
    "ts-node": "3.3.0",
    "typescript": "2.5.2"
  },
  "dependencies": {
    "@types/express": "^4.11.1",
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^4.0.1",
    "reflect-metadata": "^0.1.10",
    "sqlite3": "^3.1.10",
    "typeorm": "0.1.12"
  },
  "scripts": {
    "start": "nodemon",
    "compile": "ts-node src/index.ts"
  },
  "nodemonConfig": {
